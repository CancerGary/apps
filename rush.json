{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.50.0",
  "pnpmVersion": "6.10.3",
  "pnpmOptions": {
    "pnpmStore": "local",
    "strictPeerDependencies": false,
    "resolutionStrategy": "fast",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14.15.0",
  "suppressNodeLtsWarning": false,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": true,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "production",
      "libraries",
      "tests"
    ],
    "ignoredNpmScopes": [
      "@types",
      "@polkadot"
    ]
  },
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/Phala-Network/apps",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,
  "projects": [
    {
      "packageName": "@phala/app",
      "projectFolder": "packages/app",
      "reviewCategory": "production",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@phala/app-config",
      "projectFolder": "packages/app-config",
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@phala/utils",
      "projectFolder": "packages/utils",
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@phala/app-types",
      "projectFolder": "packages/app-types",
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@phala/react-libs",
      "projectFolder": "packages/react-libs",
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@phala/react-hooks",
      "projectFolder": "packages/react-hooks",
      "reviewCategory": "libraries"
    },
    {
      "packageName": "@phala/react-components",
      "projectFolder": "packages/react-components",
      "reviewCategory": "libraries"
    }
  ]
}